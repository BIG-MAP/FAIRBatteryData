{
    "name": "BatteryCyclingTimeSeriesData",
    "version": "0.1",
    "namespace": "http://onto-ns.com/meta",
    "description": "A minimal description of battery cycling time series data",
    "dimensions": [
        {
            "name": "n_measurements",
            "description": "Number of measurements."
        },
        {
            "name": "n_battery",
            "description": "Number of batteries being tested."
        }
    ],
    "properties": [
        {
            "name": "current",
            "type": "double",
            "dims": ["n_measurements"],
            "unit": "A",
            "description": "Electric current through the battery."
        },
        {
            "name": "voltage",
            "type": "double",
            "dims": ["n_measurements"],
            "unit": "V",
            "description": "Voltage of the battery."
        },
        {
            "name": "time_stamp",
            "type": "string",
            "dims": ["n_measurements"],
            "description": "Time stamp of the measurement."
        },
        {
            "name": "measurement_time",
            "type": "string",
            "dims": ["n_measurements"],
            "unit": "s",
            "description": "Time of the measurement relative to the start of the experiment."
        },
        {
            "name": "battery_temperature",
            "type": "double",
            "dims": ["n_measurements"],
            "description": "Temperature of the battery."
        },
        {
            "name": "environment_temperature",
            "type": "double",
            "dims": ["n_measurements"],
            "description": "Temperature of the environment."
        }
    ]
}
